@page
@model PetHealthCareSystemRazorPages.Pages.Staff.BookingTransaction.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Transaction</h4>
<hr />
<div class="row">
    <div class="col-md-4">
         <form method="post">     
             <div class="form-group">
                 <label asp-for="Transaction.PaymentDate" class="control-label"></label>
                 <p>@Model.Transaction?.PaymentDate</p>
             </div>
             <div class="form-group">
                 <label asp-for="Transaction.PaymentStaffId" class="control-label"></label>
                 <p>@Model.Transaction?.PaymentStaffId</p>
             </div>
             <div class="form-group">
                 <label asp-for="Transaction.PaymentStaffName" class="control-label"></label>
                 <p>@Model.Transaction?.PaymentStaffName</p>
             </div>
             <div class="form-group">
                 <label asp-for="Transaction.Status" class="control-label"></label>
                 <p>@Model.Transaction?.Status</p>
             </div>
             <div class="form-group">
                 <input type="submit" value="Save" class="btn btn-primary" />
             </div>
         </form>
    </div>
</div>
